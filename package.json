{
  "name": "react-localize-redux",
  "version": "4.0.0-beta.1",
  "description": "Localization library for handling translations in React",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "coverage": "jest && codecov",
    "prepublish": "npm run build",
    "test": "jest",
    "prettier": "prettier --write src/**/*",
    "upload:demo": "codesandbox ./demo",
    "types": "tsc --declaration --emitDeclarationOnly --declarationDir types/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryandrewjohnson/react-localize-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization"
  ],
  "tags": [
    "react",
    "redux",
    "i18n",
    "localization"
  ],
  "author": "Ryan Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryandrewjohnson/react-localize-redux/issues"
  },
  "homepage": "https://github.com/ryandrewjohnson/react-localize-redux#readme",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/flat": "0.0.28",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/reselect": "^2.2.0",
    "codecov": "^3.5.0",
    "codesandbox": "^1.3.8",
    "cross-env": "^3.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "immutable": "^3.8.2",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "json-loader": "^0.5.4",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^5.9.0",
    "redux": "^3.6.0",
    "rollup": "^1.13.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "roots": [
      "tests/"
    ],
    "modulePaths": [
      "src/",
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "flat": "^4.1.0",
    "hoist-non-react-statics": "^3.3.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "reselect": "^4.0.0"
  }
}
